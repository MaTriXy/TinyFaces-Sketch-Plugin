{
  "name": "tinyfaces",
  "version": "0.6.2",
  "description": "Fill content with random avatars & data",
  "homepage": "https://tinyfac.es",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "TinyFaces 👦🏼👨🏾👩🏻",
    "manifest": "src/manifest.json",
    "main": "tinyfaces.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximedegreve/TinyFaces-Sketch-Plugin.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.11"
  },
  "author": "Maxime De Greve <maximedegreve@me.com>",
  "license": "MIT"
}
